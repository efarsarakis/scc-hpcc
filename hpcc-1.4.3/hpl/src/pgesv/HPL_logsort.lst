  176.              
  177.  + r4------<    for( i = 0; i < NPROCS; i++ )
  178.    r4           {
  179.    r4              IPMAPM1[ IPMAP[i] ] = i;
  180.    r4              IPLEN[i+1]         += IPLEN[i];
  181.    r4------>    }
