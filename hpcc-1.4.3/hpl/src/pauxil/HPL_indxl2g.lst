   64.    ( IL, INB, NB, PROC, SRCPROC, NPROCS )
   65.       const int                        IL;
   66.       const int                        INB;
   67.       const int                        NB;
   68.       const int                        PROC;
   69.       const int                        SRCPROC;
   70.       const int                        NPROCS;
